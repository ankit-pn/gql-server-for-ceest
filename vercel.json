{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "api/graphql": {
        "handler": "index.handler",
        "memory": 1024,
        "maxDuration": 10,
        "runtime": "@vercel/node"
      }
    },
    "dependencies": {
        "@apollo/server": "^4.3.0",
        "apollo-server": "^3.11.1",
        "dotenv": "^16.0.3",
        "graphql": "^16.6.0",
        "mongoose": "^6.8.3",
        "uuid": "^9.0.0"
    }
  }
  